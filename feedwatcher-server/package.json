{
  "name": "telepathy-server",
  "version": "1.5.2",
  "author": "",
  "license": "MIT",
  "scripts": {
    "dependencies": "rm -fr src/common-model && cp -R ../_shared/common-model src/common-model && rm -fr src/utils-std-ts && cp -R ../_shared/utils-std-ts src/utils-std-ts",
    "//": "-------------------------------------------------------------------",
    "build": "npm run dependencies && tsc",
    "lint": "eslint src",
    "test": "jest --coverage || true",
    "test:dev": "jest --watch --coverage",
    "////": "-----------------------------------------------------------------",
    "dev": "ts-node ./src/app.ts",
    "dev:docker": "docker-packaging-config/start-dev-docker.sh || true",
    "dev:test": "NODE_ENV=dev jest --watch --config='jest.config.js'",
    "binary:alpine:x64": "npm run build && pkg dist/app.js --target node16-alpine-x64 --output ./dist/telepathy-server-linux-alpine",
    "binary:linux:x64": "npm run build && pkg dist/app.js --target node16-linux-x64 --output ./dist/telepathy-server-linux-x64",
    "binary:linux:armv6": "npm run build && pkg dist/app.js --target node14-linux-armv6 --output ./dist/telepathy-server-linux-armv6",
    "binary:linux:arm64": "npm run build && pkg dist/app.js --target arm64 --output ./dist/telepathy-server-linux-armv7"
  },
  "dependencies": {
    "@fastify/cors": "^8.0.0",
    "@opentelemetry/api": "^1.2.0",
    "@opentelemetry/auto-instrumentations-node": "^0.32.1",
    "@opentelemetry/exporter-trace-otlp-http": "^0.33.0",
    "@opentelemetry/id-generator-aws-xray": "^1.1.0",
    "@opentelemetry/resources": "^1.7.0",
    "@opentelemetry/sdk-node": "^0.33.0",
    "@opentelemetry/sdk-trace-base": "^1.7.0",
    "@opentelemetry/sdk-trace-web": "^1.7.0",
    "@opentelemetry/semantic-conventions": "^1.7.0",
    "bcrypt": "^5.0.1",
    "body-parser": "^1.19.0",
    "fastify": "^4.2.0",
    "fs-extra": "^7.0.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.20",
    "md5-file": "^4.0.0",
    "node-cron": "^2.0.0",
    "pino-pretty": "^8.1.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/fs-extra": "^9.0.13",
    "@types/jest": "^29.2.0",
    "@types/lodash": "^4.14.186",
    "@types/node": "^18.11.8",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.42.0",
    "eslint": "^8.26.0",
    "jest": "^29.2.2",
    "sinon": "^14.0.1",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  }
}
